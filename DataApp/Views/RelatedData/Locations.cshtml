@model IEnumerable<DataApp.Models.ContactLocation>
@{
    ViewData["Title"]= "Имя локации";
    Layout = "_Layout";
 }

<table class="table table-striped table-sm">
    <tr><th>ID</th><th>Имя</th></tr>
    @foreach (var s in Model) {
        <tr><td>@s.Id</td><td>@s.LocationName</td></tr>
    }
</table>
